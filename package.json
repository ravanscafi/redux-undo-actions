{
  "name": "@ravanscafi/redux-undo-actions",
  "version": "0.0.0",
  "description": "A different undo/redo approach for Redux, tracking actions instead of state.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/ravanscafi/redux-undo-actions#readme",
  "bugs": {
    "url": "https://github.com/ravanscafi/redux-undo-actions/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ravanscafi/redux-undo-actions.git"
  },
  "author": "Ravan Scafi <rscafi@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown --format esm --format cjs",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --check .",
    "format-fix": "prettier --cache --write .",
    "lint": "eslint .",
    "lint-fix": "eslint --cache --fix .",
    "commitlint": "commitlint",
    "release": "bumpp && npm publish",
    "lefthook-install": "lefthook install",
    "lefthook-run": "lefthook run pre-commit"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "bumpp": "^10.2.3",
    "eslint": "^9.34.0",
    "lefthook": "^1.12.3",
    "prettier": "^3.6.2",
    "redux": "^5.0.1",
    "tsdown": "^0.14.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "redux": ">5.0.0"
  }
}
